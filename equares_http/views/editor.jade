extends page
block vars
    - var title = 'Equares editor'

block otherCss
    link(rel='stylesheet', href='stylesheets/scheme-editor.css')
    //- link(rel='stylesheet', href='stylesheets/equarescmd.css')
    link(rel='stylesheet', href='stylesheets/jquery-linenum.css')

block otherScripts
    script(type='text/javascript' src='js-3rd/d3.v3.js')
    script(type='text/javascript' src='js/jquery-linenum.js')
    script(type='text/javascript' src='js/equaresui.js')
    script(type='text/javascript' src='js/equares.js')
    script(type='text/javascript' src='js/equares-box.js')
    script(type='text/javascript' src='js/scheme-editor.js')

block extras
    input#simulation-uploader(type='file' style='display: none;')

    div
        #running-simulation(title='Running simulation')
            p Status: <span class="status"></span>
            p Simulation output files
            #simulation-output-files

    #loading-progress-overlay
        #loading-progress
            #loading-progress-label Loading...
